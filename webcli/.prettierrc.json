{
  "printWidth": 80,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^~/const/(.*)",
    "^~/dev/(.*)",
    "^~/lib/(.*)",
    "^~(.*)",
    "^\\.(.*)"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
